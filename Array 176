#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,i, found=0;
    scanf("%d",&n);
    int doors[n];
    for(i=0;i<n;i++)
        {
        scanf("%d",& doors [i]);
        }
    int friend_address;
    scanf("%d",&friend_address);
    for(i=0;i<n;i++)
        {
        if(doors[i]==friend_address)
            {
            printf("Door Number is %03d-DN\n",i);
            found=1;
            break;
           }
        }
    if(! found)
        {
        printf("-1\n");
        }
    return 0;
    }
        

   


