#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int fibonacci(int pos)
{
    if(pos==1)
        return 0;
    if(pos==2)
        return 1;
    int a = 0,b = 1,c;
    for(int i=3;i<=pos;i++)
        {
        c=a+b;
        a=b;
        b=c;
        }
    return b;
    }
int main()
    {
    int n,m;
    float sum=0;
    scanf("%d%d",&n,&m);
    if(n<1||n>20||m<1||m>20||n>m)
        {
        printf("Invalid Input\n");
        }
    else
        {
        for(int i=n;i<=m;i++)
            {
            sum+=fibonacci(i);
            }
        printf("The Sum of Fibonacci value is %.1f\n",sum);
        }
    return 0;
    }
    
   

