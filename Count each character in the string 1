#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char str[100];
    int count[256] = {0};
    int i;
    fgets(str, sizeof(str), stdin);
    str[strcspn(str, "\n")] = 0;
    for (i = 0; str[i] != '\0'; i++)
        {
    count[(unsigned char)str[i]]++;
         }

    int printed[256] = {0};
    for (i = 0; str[i] != '\0'; i++) 
      {
    unsigned char ch = str[i];
    if (!printed[ch]) 
        {
    printf("%c   %d\n", ch, count[ch]);
    printed[ch] = 1;
          }
    }
       return 0;  
}
    
