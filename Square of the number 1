#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
struct Calculator
    {
    int num;
    void(*square)(struct Calculator);
    };
void calculateSquare(struct Calculator obj)
    {
    int result = obj.num*obj.num;
    printf("Square of %d is %d\n", obj.num, result);
    }
int main()
    {
    int input;
    scanf("%d",& input);
    if(input<1||input>9)
        {
        printf("Invalid Input\n");
        return 0;
        }
    struct Calculator calc;
    calc.num=input;
    calc.square=calculateSquare;
    calc.square(calc);
    return 0;
    }
    
  


