#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
void printNatural(int num, int current) 
{
    if (current > num)
    return;
    printf("%d ", current);
    printNatural(num, current + 1);
    } 
    int main() 
    {
    int n;
    scanf("%d", &n);
    if (n >= 1 && n <= 50)
    {
     printf("The first %d Natural Numbers are:\n", n);
     printNatural(n, 1);
        } else {
        printf("Enter a Valid Input!!!!!!!!!!!!!!!!!!!!!!!!\n");

    }
        return 0;
        }
