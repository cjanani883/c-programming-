#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

    int isVowel(char ch)
        {
        return (ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u');
        }
    int main()
        {
        char str[100],result[100];
        int i,j=0;
        fgets(str,sizeof(str),stdin);
        for(i=0;str[i]!='\0';i++)
            {
            if(!isVowel(str[i]))
                {
                result[j++]=str[i];
                }
            }
        result[j]='\0';
        printf("%s",result);
        return 0;
        }
