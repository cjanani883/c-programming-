#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int alpcount=0;
    char arr[10];
    scanf("%s",arr);
    for(int i=0;arr[i]!='\0';i++)
        {
  if(arr[i]=='a'||arr[i]=='e'||arr[i]=='i'||arr[i]=='o'||arr[i]=='u'||arr[i]=='A'||arr[i]=='E'||
     arr[i]=='I'||arr[i]=='O'||arr[i]=='U')
      {
      alpcount++;
      }
        }
    printf("count: %d",alpcount);
    return 0;
    }



