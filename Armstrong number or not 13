#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int number,original,remainder, result=0;
    scanf("%d",&number);
    original=number;
    while(original!=0)
        {
        remainder=original%10;
        result+=remainder*remainder*remainder;
        original/=10;
        }
    if(result==number && number>=100 && number<=999)
        {
        printf("Yes\n");
        }
    else
        {
        printf("No\n");
        }

    
    return 0;
}
